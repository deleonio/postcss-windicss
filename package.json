    "exit-hook": "^2.2.1"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.2",
    "@antfu/ni": "^0.5.5",
    "@types/deasync": "^0.1.1",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "bumpp": "^6.0.6",
    "eslint": "^7.23.0",
    "eslint-plugin-jest": "^24.3.2",
    "esno": "^0.5.0",
    "git-ensure": "^0.1.0",
    "jest": "^26.6.3",
    "postcss": "^8.2.9",
    "ts-jest": "^26.5.4",
    "tsup": "^4.8.21",
    "typescript": "^4.2.3",
    "windicss": "^2.5.9"
  }
